{
	"name": "vim-note",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "turbo build",
		"preview": "turbo preview",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"test:e2e": "turbo run build preview & PREVIEW_PID=$! && turbo run test:e2e && kill $PREVIEW_PID"
	},
	"devDependencies": {
		"cypress": "^14.1.0",
		"prettier": "^3.5.0",
		"turbo": "^2.4.1",
		"typescript": "5.7.3"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=18"
	}
}
